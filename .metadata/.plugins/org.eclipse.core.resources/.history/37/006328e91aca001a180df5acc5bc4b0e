import java.util.Scanner;

public class Exercicio {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//exercicio1
		
//		int x = sc.nextInt();
//		
//		if(x >= 1 || x <= 1000) {
//			for(int i = 0; i <= x; i++) {
//				if(i%2 != 0) {
//					System.out.println(i);
//				}
//			}
//		}
		//exercicio2
		int n = sc.nextInt();
		int in = 0;
		int out = 0;
		
		for(int i = 0; i < n; i++) {
			int x = sc.nextInt();
			if(x >= 10 && x <= 20) {
				in ++;
			} else {
				out ++;
			}
		}
		System.out.println(in + " in");
		System.out.println(out + " out");
		
		sc.close();
	}
	
	
	
	

}
