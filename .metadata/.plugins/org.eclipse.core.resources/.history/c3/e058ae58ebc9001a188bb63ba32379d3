import java.util.Scanner;

public class Exercicio {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//exercicio1
		
		int x = sc.nextInt();
		
		if(x >= 1 || x <= 1000) {
			for(int i = 0; i <= x; i=+2) {
				System.out.println(i);
			}
		}
		
		sc.close();
	}

}
